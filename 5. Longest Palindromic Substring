def longestPalindrome(s):
    res=""
    for i in range(len(s)):
        temp=helper(s, i, i)
        if len(temp)>len(res):
            res=temp
        temp=helper(s, i, i+1)
        if len(temp)>len(res):
            res=temp
    return res

def helper(s, a, b):
    while a>=0 and b<len(s) and s[a]==s[b]:
        a-=1
        b+=1

    return s[a+1:b]

s="babad"
print(longestPalindrome(s))
