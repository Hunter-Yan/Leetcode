class ListNode():
    def __init__(self,x):
        self.val=x
        self.next=None



def InitList(array):
    root = ListNode(0)
    result = root
    for i in array:
        root.next = ListNode(i)
        root = root.next
    return result.next

def addTwoNumbers(l1, l2):
    output = ListNode(0)
    result = output
    carry = 0
    while l1 or l2 or carry:
        v1 = v2 = 0
        if l1:
            v1 = l1.val
            l1 = l1.next
        if l2:
            v2 = l2.val
            l2 = l2.next
        s = v1 + v2
        val = s % 10 + carry
        carry = s // 10
        output.next = ListNode(val)
        output = output.next

    return result.next

l1=InitList([2,4,3])
l2=InitList([5,6,4])
k = addTwoNumbers(l1, l2)
print(k.next.next.val)
